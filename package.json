{
  "name": "obsidian-parser-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "setup": "npm install && next build",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint && npx tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepare": "bash scripts/setup-hooks.sh",
    "clean": "rm -rf .next && rm -rf node_modules/.cache && rm -rf coverage",
    "clean:next": "rm -rf .next",
    "clean:cache": "rm -rf node_modules/.cache && rm -rf .next/cache",
    "clean:all": "npm run clean && rm -rf node_modules && npm install",
    "fresh-build": "npm run clean:next && npm run build",
    "fresh-dev": "npm run clean:next && npm run dev"
  },
